<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.support.ongoing">


<application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="android.support.ongoingapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="android.support.ongoingapp.SignUp" />
        <activity android:name="android.support.ongoingapp.Pokemons" />
        <activity android:name="android.support.ongoingapp.DetailActivity"></activity>
    </application>

</manifest>